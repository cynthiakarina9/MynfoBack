@model IEnumerable<MynfoBack.Models.DeviceUser>

@{
    ViewBag.Title = "Index";
}

<h2>DeviceUsers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NickName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NickName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DeviceUserID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DeviceUserID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DeviceUserID })
        </td>
    </tr>
}

</table>
